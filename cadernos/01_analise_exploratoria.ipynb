Copiar c√≥digo
ativos = {
    "PETR4": "PETR4.SA",
    "BBAS3": "BBAS3.SA",
    "VALE3": "VALE3.SA",
    "DOLAR": "USDBRL=X",
    "WINFUT": "^BVSP"
}

dados = {}
for nome, ticker in ativos.items():
    df = yf.download(ticker, period="1y", interval="1d", auto_adjust=True, progress=False)
    df["Ativo"] = nome
    dados[nome] = df

# Unir todos em um DataFrame √∫nico
df_all = pd.concat(dados.values(), keys=dados.keys())
df_all.reset_index(inplace=True)
